# SCons build description file.
# See http://scons.org/.

import os
from os.path import join

# Import the compilation context from the calling SConstruct file.
Import("context")

# Setup shorthands.
env = context.environment
config = context.configuration
files = [ "src/" + file for file in context.files["double_conversion"] ]
shared = context.shared["objects"]

# Setup compilation of the library.
library = env.Library("double_conversion", files)

Return("library")
