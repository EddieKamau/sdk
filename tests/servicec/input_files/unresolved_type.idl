service Painter {
  void draw(Circle* c);
  void refresh();
}

struct Circle {
  int radius;  // There is no int type, just int8, int16, etc.
  Point2d position;
}

struct Point2d {
  int x;  // There is no int type, just int8, int16, etc.
  int y;  // There is no int type, just int8, int16, etc.
}
